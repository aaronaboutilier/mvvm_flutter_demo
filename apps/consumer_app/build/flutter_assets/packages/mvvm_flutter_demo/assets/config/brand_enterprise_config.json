{
  "brand": {
    "appName": "Enterprise Task Manager",
    "logoPath": "assets/brand/enterprise_logo.png",
    "primaryColorHex": "#1976D2",
    "secondaryColorHex": "#424242",
    "accentColorHex": "#FF9800",
    "fontFamily": "Roboto",
    "websiteUrl": "https://enterprise.example.com",
    "supportEmail": "enterprise-support@example.com",
    "customAssets": {
      "splashScreen": "assets/brand/enterprise_splash.png",
      "watermark": "assets/brand/enterprise_watermark.png"
    }
  },
  "theme": {
    "themeMode": "light",
    "textScaleFactor": 1.0,
    "visualDensity": {
      "horizontal": 0.0,
      "vertical": 0.0
    },
    "useSystemAccentColor": false,
    "useMaterial3": true,
    "customColors": {
      "warningColor": "#FF5722",
      "successColor": "#4CAF50"
    }
  },
  "accessibility": {
    "enableScreenReader": true,
    "enableVoiceGuidance": false,
    "reduceAnimations": false,
    "increasedContrast": false,
    "largerTouchTargets": true,
    "minimumTouchSize": 48.0,
    "enableSemanticLabels": true,
    "enableHapticFeedback": true
  },
  "features": {
    "enableUserProfiles": true,
    "enableItemReordering": true,
    "enableColorCustomization": false,
    "enableDataExport": true,
    "enableOfflineMode": true,
    "enableAnalytics": true,
    "enablePushNotifications": true,
    "customFeatures": {
      "enableTeamManagement": true,
      "enableReporting": true,
      "enableAuditLog": true,
      "enableSSO": true
    }
  },
  "localization": {
    "languageCode": "en",
    "countryCode": "US",
    "useDeviceLocale": false,
    "dateFormat": "yyyy-MM-dd",
    "timeFormat": "24h",
    "numberFormat": "en_US"
  }
}